<CHART_XML>
    <chart showplotborder="1" piefillalpha="60" pieborderthickness="2" hoverfillcolor="#CCCCCC" piebordercolor="#FFFFFF" theme="fusion" pieRadius="85%">
        <category>
	        <categoryL2 label="!@#SERIES_AXIS!@#" color="#COLOR_CODE#">
	            <categoryL3 label="!@#X_AXIS!@#" tooltext="!@#X_AXIS!@# : !@#Y_AXIS!@#" value="!@#Y_AXIS!@#" color="#COLOR_CODE#" ddkey="!@#DRILLDOWN_KEY!@#"></categoryL3>
	        </categoryL2>
	    </category>    
    </chart>
    <Description>Multi Level Pie</Description>
    <SWF_FILE_NAME>MultiLevelPie</SWF_FILE_NAME>
    <REPEATING_TAG>                  categoryL2,categoryL3          </REPEATING_TAG>
    <TAB_PROPERTIES>
        <X_AXIS>
            <Multi_Flag>N</Multi_Flag>
            <selection>D</selection>
            <enable>Y</enable>
        </X_AXIS>
        <Y_AXIS>
            <Multi_Flag>Y</Multi_Flag>
            <selection>M</selection>
            <enable>Y</enable>
        </Y_AXIS>
        <Z_AXIS>
            <Multi_Flag></Multi_Flag>
            <selection></selection>
            <enable>N</enable>
        </Z_AXIS>
        <SERIES_AXIS>
            <Multi_Flag>N</Multi_Flag>
            <selection>D</selection>
            <enable>Y</enable>
        </SERIES_AXIS>
    </TAB_PROPERTIES>
</CHART_XML>